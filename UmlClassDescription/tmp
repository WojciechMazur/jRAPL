- samples: ArrayList<Long> 
- samplingRate: int 
- exit: volatile boolean 
- runtime: static final Runtime 
- t: Thread 
- birth: Instant 
- death: Instant 
- lifetime: Duration 

+ AsyncMemoryMonitor()
- memoryUsed(): long 
